###  http://www.w3.org/ns/prov#generated
<http://www.w3.org/ns/prov#generated> rdf:type owl:ObjectProperty ;
                                      rdfs:domain :Event ;
                                      rdfs:range :Data ;
                                      rdfs:comment "Relaciona un evento con los datos generados por el mismo."@es ,
                                                   "Relates an event to the generated data by the same."@en ;
                                      rdfs:label "Genera"@es ,
                                                 "Generates"@en .


###  https://w3id.org/optimar#adjusts
:adjusts rdf:type owl:ObjectProperty ;
         rdfs:domain :Actor ;
         rdfs:range [ rdf:type owl:Class ;
                      owl:unionOf ( :Component
                                    :PitchSystem
                                  )
                    ] ;
         rdfs:comment "Indica que un actor realiza ajustes sobre un componente o sistema de pitch."@es ,
                      "Indicas that an actor makes adjustments on a component or pitch system."@en ;
         rdfs:label "Adjusts"@en ,
                    "Ajusta"@es .


###  https://w3id.org/optimar#analyzes
:analyzes rdf:type owl:ObjectProperty ;
          rdfs:domain :Actor ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Data
                                     :Failure
                                   )
                     ] ;
          rdfs:comment "Relaciona a un actor con los datos o fallos que estudia o interpreta."@es ,
                       "Relates an actor to the data or failures that it studies or interprets."@en ;
          rdfs:label "Analiza"@es ,
                     "Analyzes"@en .


###  https://w3id.org/optimar#causedBy
:causedBy rdf:type owl:ObjectProperty ;
          rdfs:domain :Event ;
          rdfs:range :Failure ;
          rdfs:comment "Relaciona un evento con el fallo que lo provocó."@es ,
                       "Relates an event with the failure that caused it."@en ;
          rdfs:label "Causado por"@es ,
                     "Caused by"@en .


###  https://w3id.org/optimar#connectedTo
:connectedTo rdf:type owl:ObjectProperty ;
             rdfs:domain :Component ;
             rdfs:range :Component ;
             rdfs:comment "Indica que un componente está conectado a otro."@es ,
                          "Indicates that a component is connected to another."@en ;
             rdfs:label "Conectado a"@es ,
                        "Connected to"@en .


###  https://w3id.org/optimar#detectedBy
:detectedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Event ;
            rdfs:range :Sensor ;
            rdfs:comment "Relaciona un evento con el sensor que lo detectó."@es ,
                         "Relates an event to the sensor that detected it."@en ;
            rdfs:label "Detectado por"@es ,
                       "Detected by"@en .


###  https://w3id.org/optimar#detectsCorrosion
:detectsCorrosion rdf:type owl:ObjectProperty ;
                  rdfs:domain :Sensor ;
                  rdfs:range :Corrosion ;
                  rdfs:comment "Relaciona un sensor con la medición de corrosión detectada."@es ,
                               "Relates a sensor with the detected corrosion measurement."@en ;
                  rdfs:label "Detecta corrosión"@es ,
                             "Detects corrosion"@en .


###  https://w3id.org/optimar#diameter
:diameter rdf:type owl:ObjectProperty ;
          rdfs:domain :Component ;
          rdfs:range <http://qudt.org/schema/qudt/QuantityValue> ;
          rdfs:comment "Diameter of the component measured in QUDT units."@en ,
                       "Diámetro del componente medido en unidades QUDT."@es ;
          rdfs:label "Diameter"@en ,
                     "Diámetro"@es .


###  https://w3id.org/optimar#failureObservation
:failureObservation rdf:type owl:ObjectProperty ;
                    rdfs:domain :FailureEvent ;
                    rdfs:range :Observation ;
                    rdfs:comment "Relación entre un evento de fallo y la observación realizada por un sensor."@es ,
                                 "Relationship between a failure event and the observation made by a sensor."@en ;
                    rdfs:label "Failure observation"@en ,
                               "Observación de fallo"@es .


###  https://w3id.org/optimar#hasFailureCause
:hasFailureCause rdf:type owl:ObjectProperty ;
                 rdfs:domain :Failure ;
                 rdfs:range :EnvironmentalCondition ;
                 rdfs:comment "Indica que un fallo tiene su origen en una condición ambiental específica."@es ,
                              "Indicates that a failure is caused by a specific environmental condition."@en ;
                 rdfs:label "Causa de fallo"@es ,
                            "Failure cause"@en .


###  https://w3id.org/optimar#hasRole
:hasRole rdf:type owl:ObjectProperty ;
         rdfs:domain :Actor ;
         rdfs:range :Role ;
         rdfs:comment "Indica el rol que desempeña un actor dentro del sistema."@es ,
                      "Indicas the role that an actor performs within the system."@en ;
         rdfs:label "Has role"@en ,
                    "Tiene rol"@es .


###  https://w3id.org/optimar#height
:height rdf:type owl:ObjectProperty ;
        rdfs:domain :Component ;
        rdfs:range <http://qudt.org/schema/qudt/QuantityValue> ;
        rdfs:comment "Altura del componente expresada como un valor con unidad QUDT."@es ,
                     "Height of the component expressed as a QUDT quantity value."@en ;
        rdfs:label "Altura"@es ,
                   "Height"@en .


###  https://w3id.org/optimar#houses
:houses rdf:type owl:ObjectProperty ;
