        rdfs:range :Component ;
        rdfs:comment "Indica que un componente contiene o aloja fisicamente a otro en su estructura."@es ,
                     "Indicates that a component contains or houses physically another in its structure."@en ;
        rdfs:label "Aloja"@es ,
                   "Houses"@en .


