        <rdfs:comment xml:lang="en">Relationship between a failure event and the observation made by a sensor.</rdfs:comment>
        <rdfs:label xml:lang="en">Failure observation</rdfs:label>
        <rdfs:label xml:lang="es">Observación de fallo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#hasFailureCause -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#hasFailureCause">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Failure"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#EnvironmentalCondition"/>
