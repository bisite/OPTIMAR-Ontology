        <rdfs:comment xml:lang="en">Mass of the component expressed as a QUDT quantity value</rdfs:comment>
        <rdfs:label xml:lang="es">Masa</rdfs:label>
        <rdfs:label xml:lang="en">Mass</rdfs:label>
    </owl:ObjectProperty>
    


