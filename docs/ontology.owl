        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-06</dc:date>
        <dc:description xml:lang="es">Esta ontología describe los componentes, fallos, sensores y eventos relacionados con las plataformas eólicas offshore.</dc:description>
        <dc:description xml:lang="en">This ontology describes the components, failures, sensors, and events related to offshore wind platforms.</dc:description>
        <dc:language>en</dc:language>
        <dc:name xml:lang="en">OPTiMAR Offshore Wind Ontology</dc:name>
        <dc:name xml:lang="es">Ontología Offshore OPTiMAR</dc:name>
        <dc:title xml:lang="en">Offshore Wind Power Platform Ontology for OPTiMAR</dc:title>
        <dc:title xml:lang="es">Ontología de Plataforma Eólica Offshore para OPTiMAR</dc:title>
        <terms:abstract xml:lang="es">Ontologia que modela los elementos estructurales, sensores, eventos y fallos de plataformas eólicas offshore, incluyendo datos de medición y actores involucrados.</terms:abstract>
        <terms:abstract xml:lang="en">Ontology modeling structural elements, sensors, events, and failures in offshore wind platforms, including measurement data and involved actors.</terms:abstract>
        <terms:bibliographicCitation xml:lang="en">Flores, A., Montero, A., Garcia, O., &amp; Aceta, C. (2025). Offshore Wind Power Platform Ontology for OPTiMAR. DOI: 10.1234/abcd</terms:bibliographicCitation>
        <terms:bibliographicCitation xml:lang="es">Flores, A., Montero, A., Garcia, O., &amp; Aceta, C. (2025). Ontología de Plataforma Eólica Offshore para OPTiMAR. DOI: 10.1234/abcd</terms:bibliographicCitation>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-30</terms:created>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-09-30</terms:issued>
        <terms:license rdf:resource="https://opensource.org/licenses/MIT"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-10-06</terms:modified>
        <terms:publisher xml:lang="es">Universidad de Salamanca, Centro Tecnológico y de Investigación - Tekniker</terms:publisher>
        <terms:publisher xml:lang="en">University of Salamanca, Technology and Research Centre - Tekniker</terms:publisher>
        <terms:rights xml:lang="es">Este trabajo se distribuye bajo la licencia MIT. Se permite copiar, modificar y distribuir bajo los términos de dicha licencia.</terms:rights>
        <terms:rights xml:lang="en">This work is distributed under the MIT License. It may be copied, modified, and distributed under the terms of this license.</terms:rights>
        <terms:status xml:lang="es">estable</terms:status>
        <terms:status xml:lang="en">stable</terms:status>
        <vann:preferredNamespacePrefix>ex</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="https://w3id.org/optimar#"/>
        <owl:backwardCompatibleWith rdf:resource="https://w3id.org/optimar/0.2"/>
        <owl:versionInfo xml:lang="en">Version 1.0 of the Ontology</owl:versionInfo>
        <owl:versionInfo xml:lang="es">Versión 1.0 de la ontología</owl:versionInfo>
        <foaf:homepage rdf:resource="https://w3id.org/optimar"/>
