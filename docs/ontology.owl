        <rdfs:label xml:lang="en">Made by sensor</rdfs:label>
        <rdfs:label xml:lang="es">Realizada por sensor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#manages -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#manages">
