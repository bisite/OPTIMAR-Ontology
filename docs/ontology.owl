    <!-- http://www.w3.org/ns/prov#generated -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#generated">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Event"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Data"/>
        <rdfs:comment xml:lang="es">Relaciona un evento con los datos generados por el mismo.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates an event to the generated data by the same.</rdfs:comment>
        <rdfs:label xml:lang="es">Genera</rdfs:label>
        <rdfs:label xml:lang="en">Generates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#adjusts -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#adjusts">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Actor"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/optimar#Component"/>
                    <rdf:Description rdf:about="https://w3id.org/optimar#PitchSystem"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="es">Indica que un actor realiza ajustes sobre un componente o sistema de pitch.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicas that an actor makes adjustments on a component or pitch system.</rdfs:comment>
        <rdfs:label xml:lang="en">Adjusts</rdfs:label>
        <rdfs:label xml:lang="es">Ajusta</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#analyzes -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#analyzes">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Actor"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/optimar#Data"/>
                    <rdf:Description rdf:about="https://w3id.org/optimar#Failure"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="es">Relaciona a un actor con los datos o fallos que estudia o interpreta.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates an actor to the data or failures that it studies or interprets.</rdfs:comment>
        <rdfs:label xml:lang="es">Analiza</rdfs:label>
        <rdfs:label xml:lang="en">Analyzes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#causedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#causedBy">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Event"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Failure"/>
        <rdfs:comment xml:lang="es">Relaciona un evento con el fallo que lo provocó.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates an event with the failure that caused it.</rdfs:comment>
        <rdfs:label xml:lang="es">Causado por</rdfs:label>
        <rdfs:label xml:lang="en">Caused by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#connectedTo -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#connectedTo">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Component"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Component"/>
        <rdfs:comment xml:lang="es">Indica que un componente está conectado a otro.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates that a component is connected to another.</rdfs:comment>
        <rdfs:label xml:lang="es">Conectado a</rdfs:label>
        <rdfs:label xml:lang="en">Connected to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#detectedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#detectedBy">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Event"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Sensor"/>
        <rdfs:comment xml:lang="es">Relaciona un evento con el sensor que lo detectó.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates an event to the sensor that detected it.</rdfs:comment>
        <rdfs:label xml:lang="es">Detectado por</rdfs:label>
        <rdfs:label xml:lang="en">Detected by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#detectsCorrosion -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#detectsCorrosion">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Sensor"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Corrosion"/>
        <rdfs:comment xml:lang="es">Relaciona un sensor con la medición de corrosión detectada.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relates a sensor with the detected corrosion measurement.</rdfs:comment>
        <rdfs:label xml:lang="es">Detecta corrosión</rdfs:label>
        <rdfs:label xml:lang="en">Detects corrosion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#diameter -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#diameter">
