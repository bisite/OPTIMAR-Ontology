        <rdfs:label xml:lang="en">Diameter</rdfs:label>
        <rdfs:label xml:lang="es">Diámetro</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#failureObservation -->


