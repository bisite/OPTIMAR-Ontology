        <rdfs:comment xml:lang="en">Indicates that a failure is caused by a specific environmental condition.</rdfs:comment>
        <rdfs:label xml:lang="es">Causa de fallo</rdfs:label>
        <rdfs:label xml:lang="en">Failure cause</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#hasRole -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#hasRole">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Actor"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Role"/>
        <rdfs:comment xml:lang="es">Indica el rol que desempeña un actor dentro del sistema.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicas the role that an actor performs within the system.</rdfs:comment>
        <rdfs:label xml:lang="en">Has role</rdfs:label>
        <rdfs:label xml:lang="es">Tiene rol</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#height -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#height">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Component"/>
        <rdfs:range rdf:resource="http://qudt.org/schema/qudt/QuantityValue"/>
        <rdfs:comment xml:lang="es">Altura del componente expresada como un valor con unidad QUDT.</rdfs:comment>
        <rdfs:comment xml:lang="en">Height of the component expressed as a QUDT quantity value.</rdfs:comment>
        <rdfs:label xml:lang="es">Altura</rdfs:label>
        <rdfs:label xml:lang="en">Height</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#houses -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#houses">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Component"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Component"/>
        <rdfs:comment xml:lang="es">Indica que un componente contiene o aloja fisicamente a otro en su estructura.</rdfs:comment>
        <rdfs:comment xml:lang="en">Indicates that a component contains or houses physically another in its structure.</rdfs:comment>
        <rdfs:label xml:lang="es">Aloja</rdfs:label>
        <rdfs:label xml:lang="en">Houses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#isSupportedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#isSupportedBy">
        <rdfs:domain rdf:resource="https://w3id.org/optimar#Component"/>
        <rdfs:range rdf:resource="https://w3id.org/optimar#Component"/>
