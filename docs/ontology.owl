        <rdfs:label xml:lang="en">Located in</rdfs:label>
        <rdfs:label xml:lang="es">Ubicado en</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/optimar#madeBySensor -->


    <owl:ObjectProperty rdf:about="https://w3id.org/optimar#madeBySensor">
        <rdfs:domain rdf:resource="https://www.w3.org/ns/sosa/Observation"/>
        <rdfs:range rdf:resource="https://www.w3.org/ns/sosa/Sensor"/>
        <rdfs:comment xml:lang="es">Relaciona una observación con el sensor que la realizó.</rdfs:comment>
