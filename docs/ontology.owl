        <rdfs:comment xml:lang="en">Indicates that a component is structurally or functionally supported by another.</rdfs:comment>
        <rdfs:label xml:lang="es">Es soportado por</rdfs:label>
        <rdfs:label xml:lang="en">Is supported by</rdfs:label>
    </owl:ObjectProperty>
    


