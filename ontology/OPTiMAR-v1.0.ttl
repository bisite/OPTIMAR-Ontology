##########################################################
# Main classes and subclasses
##########################################################

ex:WindPowerPlatform a owl:Class ;
    rdfs:label "Plataforma eólica offshore"@es ;
    rdfs:label "Offshore Wind Power Platform"@en ;
    rdfs:comment "Una plataforma eólica offshore que genera energía electrica a partir del viento."@es ;
    rdfs:comment "An offshore wind power platform that generates electricity from wind."@en .

ex:Component a owl:Class ;
    rdfs:label "Componente"@es ;
    rdfs:label "Component"@en ;
    rdfs:comment "Un componente de una plataforma eólica offshore, como las palas, torres, sistema de pitch, etc."@es ;
    rdfs:comment "An component of an offshore wind power platform, such as blades, towers, pitch system, etc."@en .

ex:Sensor a owl:Class ;
    rdfs:subClassOf sosa:Sensor ;
    rdfs:label "Sensor"@es ;
    rdfs:label "Sensor"@en ;
    rdfs:comment "Un dispositivo que mide o monitorea parámetros de la plataforma eólica, como viento, corrosión, vibraciones, etc."@es ;
    rdfs:comment "A device that measures or monitors parameters of the wind platform, such as wind, corrosion, vibrations, etc."@en .

ex:Data a owl:Class ;
    rdfs:label "Dato"@es ;
    rdfs:label "Data"@en ;
    rdfs:comment "Una clase general para representar los datos de medición recolectados por sensores u otros sistemas."@es ;
    rdfs:comment "A class for representing the measurement data collected by sensors or other systems."@en .

ex:Failure a owl:Class ;
    rdfs:label "Fallo"@es ;
    rdfs:label "Failure"@en ;
    rdfs:comment "Una clase que representa fallos identificados en la plataforma eólica."@es ;
    rdfs:comment "A class that represents identified failures in the wind platform."@en .

ex:Maintenance a owl:Class ;
    rdfs:label "Mantenimiento"@es ;
    rdfs:label "Maintenance"@en ;
    rdfs:comment "Una clase que representa el mantenimiento de la plataforma eólica."@es ;
    rdfs:comment "A class that represents the maintenance of the wind platform."@en ;
    ex:performedBy ex:MaintenanceOperator .

ex:Event a owl:Class ;
    rdfs:label "Evento"@es ;
    rdfs:label "Event"@en ;
    rdfs:comment "Un evento que ocurre en la plataforma eólica, como la detección de un fallo."@es ;
    rdfs:comment "An event that occurs in the offshore wind power platform, such as a failure detection."@en .

ex:Actor a owl:Class ;
    rdfs:label "Actor"@es ;
    rdfs:label "Actor"@en ;
    rdfs:comment "Un actor que interactúa o gestiona la plataforma eólica."@es ;
    rdfs:comment "An actor that interacts or manages the offshore wind power platform."@en .

ex:Role a owl:Class ;
    rdfs:label "Rol"@es ;
    rdfs:label "Role"@en ;
    rdfs:comment "Los roles de los actores que gestionan, operan o mantienen la plataforma eólica."@es ;
    rdfs:comment "The roles of the actors that manage, operate or maintain the offshore wind power platform."@en .

ex:Location a owl:Class ;
    rdfs:label "Ubicación"@es ;
    rdfs:label "Location"@en ;
    rdfs:comment "La ubicación geográfica de la plataforma eólica."@es ;
    rdfs:comment "The geographic location of the wind platform."@en .

ex:EnvironmentalCondition a owl:Class ;
    rdfs:label "Condición ambiental"@es ;
    rdfs:label "Environmental Condition"@en ;
    rdfs:comment "Las condiciones ambientales que afectan a la plataforma eólica, como la velocidad del viento, la temperatura, etc."@es ;
    rdfs:comment "The environmental conditions that affect the offshore wind power platform, such as wind speed, temperature, etc."@en .

##########################################################
# Structural components
##########################################################
ex:StructuralComponent a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:unionOf ( ex:Base ex:MooringSystem ex:FloatingSubstructure )
    ] ;
    rdfs:label "Componentes estructurales"@es ;
    rdfs:label "Structural Components"@en ;
    rdfs:comment "Los componentes estructurales que componen la infraestructura básica de la plataforma eólica."@es ;
    rdfs:comment "The structural components that make up the basic infrastructure of the wind platform."@en .

ex:TurbineComponent a owl:Class ;
    owl:equivalentClass [
        a owl:Class ;
        owl:unionOf ( ex:Hub ex:Nacelle ex:Blade ex:Tower )
    ] ;
    rdfs:label "Componentes de turbina"@es ;
    rdfs:label "Turbine Components"@en ;
    rdfs:comment "Los componentes de la turbina eólica que generan energía a partir del viento."@es ;
    rdfs:comment "The turbine components that generate energy from the wind."@en .

ex:Turbine a owl:Class ;
    rdfs:subClassOf ex:Component ;
    rdfs:label "Turbina"@es ;
    rdfs:label "Turbine"@en ;
    rdfs:comment "Una turbina de viento que forma parte de la plataforma eólica."@es ;
    rdfs:comment "A wind turbine that is part of the wind platform."@en .

ex:Hub a owl:Class ;
    rdfs:subClassOf ex:TurbineComponent ;
    rdfs:label "Hub"@es ;
    rdfs:label "Hub"@en ;
    rdfs:comment "La parte central de la turbina donde se conectan las palas."@es ;
    rdfs:comment "The central part of the turbine where the blades are connected."@en .

ex:Nacelle a owl:Class ;
    rdfs:subClassOf ex:TurbineComponent ;
    rdfs:label "Nacelle"@es ;
    rdfs:label "Nacelle"@en ;
    rdfs:comment "El compartimento de la turbina que aloja el generador y otros componentes mecánicos."@es ;
    rdfs:comment "The turbine component that houses the generator and other mechanical components."@en .

ex:Blade a owl:Class ;
    rdfs:subClassOf ex:TurbineComponent ;
    rdfs:label "Pala"@es ;
    rdfs:label "Blade"@en ;
    rdfs:comment "Las palas de la turbina que capturan la energía eólica."@es ;
    rdfs:comment "The blades of the turbine that capture the wind energy."@en .

ex:Tower a owl:Class ;
    rdfs:subClassOf ex:TurbineComponent ;
    rdfs:label "Torre"@es ;
    rdfs:label "Tower"@en ;
    rdfs:comment "La estructura que sostiene la turbina eólica."@es ;
    rdfs:comment "The structure that supports the wind turbine."@en .

ex:Base a owl:Class ;
    rdfs:subClassOf ex:StructuralComponent ;
    rdfs:label "Base (subestructura flotante)"@es ;
    rdfs:label "Base (floating substructure)"@en ;
    rdfs:comment "La base que soporta la plataforma flotante."@es ;
    rdfs:comment "The base that supports the floating platform."@en .

ex:MooringSystem a owl:Class ;
    rdfs:subClassOf ex:StructuralComponent ;
    rdfs:label "Sistema de amarre"@es ;
    rdfs:label "Mooring System"@en ;
    rdfs:comment "El sistema de amarre que mantiene la plataforma flotante en su ubicación."@es ;
    rdfs:comment "The mooring system that keeps the floating platform in place."@en .

ex:FloatingSubstructure a owl:Class ;
    rdfs:subClassOf ex:StructuralComponent ;
    rdfs:label "Subestructura flotante"@es ;
    rdfs:label "Floating Substructure"@en ;
    rdfs:comment "La subestructura flotante que permite que la plataforma sea estable en el mar."@es ;
    rdfs:comment "The floating substructure that allows the platform to be stable at sea."@en .

ex:FloatingPlatform a owl:Class ;
    rdfs:subClassOf ex:Component ;
    rdfs:label "Plataforma flotante"@es ;
    rdfs:label "Floating Platform"@en ;
    rdfs:comment "Una plataforma flotante que sirve de base para la turbina eólica."@es ;
    rdfs:comment "A floating platform that serves as a base for the wind turbine."@en .

ex:PitchSystem a owl:Class ;
    rdfs:subClassOf ex:Component ;
    rdfs:label "Sistema de pitch"@es ;
    rdfs:label "Pitch System"@en ;
    rdfs:comment "El sistema que ajusta el ángulo de las palas de la turbina."@es ;
    rdfs:comment "The system that adjusts the angle of the blades of the turbine."@en .


##########################################################
# Tasks
##########################################################
ex:Task a owl:Class ;
    rdfs:label "Tarea"@es ;
    rdfs:label "Task"@en ;
    rdfs:comment "Una tarea general relacionada con la operación, supervisión o mantenimiento de la plataforma eólica."@es ;
    rdfs:comment "A general task related to the operation, monitoring, or maintenance of the wind platform."@en .

ex:MonitoringTask a owl:Class ;
    rdfs:subClassOf ex:Task ;
    rdfs:label "Tarea de monitoreo"@es ;
    rdfs:label "Monitoring Task"@en ;
    rdfs:comment "Una tarea destinada a supervisar el estado de la plataforma mediante sensores y sistemas de monitorización."@es ;
    rdfs:comment "A task aimed at supervising the condition of the platform through sensors and monitoring systems."@en .

##########################################################
# Observations
##########################################################
ex:Observation a owl:Class ;
    rdfs:subClassOf sosa:Observation ;
    rdfs:label "Observación"@es ;
    rdfs:label "Observation"@en ;
    rdfs:comment "El resultado de registrar un fenómeno o condición, normalmente a través de un sensor o sistema de monitorización."@es ;
    rdfs:comment "The result of recording a phenomenon or condition, usually through a sensor or monitoring system."@en ;
    ex:madeBySensor sosa:Sensor .

##########################################################
# Actors
##########################################################
ex:MaintenanceOperator a owl:Class ;
    rdfs:subClassOf ex:Actor ;
    rdfs:label "Operador de mantenimiento"@es ;
    rdfs:label "Maintenance Operator"@en ;
    rdfs:comment "Persona encargada de realizar el mantenimiento preventivo y correctivo en la plataforma."@es ;
    rdfs:comment "Person responsible for carrying out preventive and corrective maintenance on the platform."@en .

ex:PlatformActuator a owl:Class ;
    rdfs:subClassOf ex:Actor ;
    rdfs:label "Actuador de plataforma"@es ;
    rdfs:label "Platform Actuator"@en ;
    rdfs:comment "Encargado de realizar ajustes sobre la plataforma, como ajustes mecánicos o hidráulicos."@es ;
    rdfs:comment "Responsible for making adjustments on the platform, such as mechanical or hydraulic adjustments."@en .

ex:CloudAdministrator a owl:Class ;
    rdfs:subClassOf ex:Actor ;
    rdfs:label "Administrador de nube"@es ;
    rdfs:label "Cloud Administrator"@en ;
    rdfs:comment "Responsable de la administración y seguridad de los servicios en la nube que procesan los datos de la plataforma."@es ;
    rdfs:comment "Responsible for managing and securing cloud services that process data from the platform."@en .

ex:DataAnalyst a owl:Class ;
    rdfs:subClassOf ex:Actor ;
    rdfs:label "Analista de datos"@es ;
    rdfs:label "Data Analyst"@en ;
    rdfs:comment "Especialista que analiza los datos recolectados para detectar patrones, fallos y oportunidades de optimización."@es ;
    rdfs:comment "Specialist who analyzes collected data to detect patterns, failures, and optimization opportunities."@en .

ex:ParkOwner a owl:Class ;
    rdfs:subClassOf ex:Actor ;
    rdfs:label "Propietario de parque"@es ;
    rdfs:label "Park Owner"@en ;
    rdfs:comment "Entidad o persona propietaria del parque eólico offshore y responsable de su operación general."@es ;
    rdfs:comment "Entity or individual owning the offshore wind farm and responsible for its overall operation."@en .

##########################################################
# Roles
##########################################################
ex:TechnicalRole a owl:Class ;
    rdfs:subClassOf ex:Role ;
    rdfs:label "Rol técnico"@es ;
    rdfs:label "Technical Role"@en ;
    rdfs:comment "Rol asociado a actividades técnicas como operación, reparación y supervisión de sistemas."@es ;
    rdfs:comment "Role associated with technical activities such as operation, repair, and system supervision."@en .

ex:ManagementRole a owl:Class ;
    rdfs:subClassOf ex:Role ;
    rdfs:label "Rol de gestión"@es ;
    rdfs:label "Management Role"@en ;
    rdfs:comment "Rol encargado de la coordinación, planificación y gestión de recursos en la plataforma eólica."@es ;
    rdfs:comment "Role responsible for coordination, planning, and resource management in the wind platform."@en .

ex:AnalyticalRole a owl:Class ;
    rdfs:subClassOf ex:Role ;
    rdfs:label "Rol analítico"@es ;
    rdfs:label "Analytical Role"@en ;
    rdfs:comment "Rol orientado al analisis de datos y generación de información útil para la toma de decisiones."@es ;
    rdfs:comment "Role focused on data analysis and generation of useful information for decision-making."@en .

##########################################################
# Sensors and monitoring systems
##########################################################
ex:CorrosionCamera a owl:Class ;
    rdfs:subClassOf ex:Sensor ;
    rdfs:label "Cámara corrosión"@es ;
    rdfs:label "Corrosion Camera"@en ;
    rdfs:comment "Cámara utilizada para detectar y monitorear corrosión en la estructura de la plataforma."@es ;
    rdfs:comment "Camera used to detect and monitor corrosion in the platform structure."@en ;
    owl:disjointWith ex:StructuralCamera ;
    owl:disjointWith ex:Anemometer ;
    owl:disjointWith ex:DynamicMonitor .

ex:StructuralCamera a owl:Class ;
    rdfs:subClassOf ex:Sensor ;
    rdfs:label "Cámara estructural"@es ;
    rdfs:label "Structural Camera"@en ;
    rdfs:comment "Cámara dedicada a la inspección visual del estado estructural de la plataforma."@es ;
    rdfs:comment "Camera dedicated to visual inspection of the structural condition of the platform."@en ;
    owl:disjointWith ex:CorrosionCamera ;
    owl:disjointWith ex:Anemometer ;
    owl:disjointWith ex:DynamicMonitor .

ex:Anemometer a owl:Class ;
    rdfs:subClassOf ex:Sensor ;
    rdfs:label "Anemómetro"@es ;
    rdfs:label "Anemometer"@en ;
    rdfs:comment "Sensor que mide la velocidad y dirección del viento en el parque eólico."@es ;
    rdfs:comment "Sensor that measures wind speed and direction in the wind farm."@en .

ex:DynamicMonitor a owl:Class ;
    rdfs:subClassOf ex:Sensor ;
    rdfs:label "Monitor dinámico"@es ;
    rdfs:label "Dynamic Monitor"@en ;
    rdfs:comment "Dispositivo que mide parámetros dinámicos de la plataforma como vibraciones y movimientos."@es ;
    rdfs:comment "Device that measures dynamic parameters of the platform, such as vibrations and movements."@en .

ex:ISUMSystem a owl:Class ;
    rdfs:subClassOf ex:Sensor ;
    rdfs:label "Sistema ISUM"@es ;
    rdfs:label "ISUM System"@en ;
    rdfs:comment "Sistema especializado para la monitorización integrada de la plataforma offshore."@es ;
    rdfs:comment "Specialized system for integrated monitoring of the offshore platform."@en .

ex:GaugeSystem a owl:Class ;
    rdfs:subClassOf ex:Sensor ;
    rdfs:label "Sistema de galgas"@es ;
    rdfs:label "Gauge System"@en ;
    rdfs:comment "Sistema de galgas extensiométricas utilizado para medir tensiones y deformaciones en la estructura."@es ;
    rdfs:comment "Strain gauge system used to measure stresses and deformations in the structure."@en .

##########################################################
# Events and Failures
##########################################################
ex:FailureEvent a owl:Class ;
    rdfs:subClassOf ex:Event ,
        [ a owl:Restriction ;
            owl:onProperty ex:causedBy ;
            owl:someValuesFrom ex:Failure
        ] ;
    rdfs:label "Evento de fallo"@es ;
    rdfs:label "Failure Event"@en ;
    rdfs:comment "Evento que representa un fallo detectado en el sistema, debe estar causado por al menos un fallo. "@es ;
    rdfs:comment "Event that represents a detected failure in the system, must be caused by at least one failure."@en .

ex:CorrosionEvent a owl:Class ;
    rdfs:subClassOf ex:FailureEvent ;
    rdfs:label "Evento de Corrosión"@es ;
    rdfs:label "Corrosion Event"@en ;
    rdfs:comment "Degradación de la estructura metálica de la plataforma por efectos de corrosión."@es ;
    rdfs:comment "Degradation of the platform's metallic structure due to corrosion effects."@en ;
    owl:disjointWith ex:MaterialFatigue ;
    owl:disjointWith ex:Misalignment .

ex:MaterialFatigue a owl:Class ;
    rdfs:subClassOf ex:Failure ;
    rdfs:label "Fatiga del material"@es ;
    rdfs:label "Material Fatigue"@en ;
    rdfs:comment "Fallo en el material estructural causado por ciclos repetidos de carga."@es ;
    rdfs:comment "Structural material failure caused by repeated load cycles."@en ;
    owl:disjointWith ex:CorrosionEvent ;
    owl:disjointWith ex:Misalignment .

ex:Misalignment a owl:Class ;
    rdfs:subClassOf ex:Failure ;
    rdfs:label "Desalineación"@es ;
    rdfs:label "Misalignment"@en ;
    rdfs:comment "Fallo causado por el desajuste o falta de alineación entre los componentes de la plataforma."@es ;
    rdfs:comment "Failure caused by misalignment or lack of proper adjustment between platform components."@en ;
    owl:disjointWith ex:CorrosionEvent ;
    owl:disjointWith ex:MaterialFatigue .

ex:ComponentBreakage a owl:Class ;
    rdfs:subClassOf ex:Failure ;
    rdfs:label "Rotura de componente"@es ;
    rdfs:label "Component Breakage"@en ;
    rdfs:comment "Fallo originado por la fractura o rotura de un componente de la plataforma eólica."@es ;
    rdfs:comment "Failure caused by the fracture or breakage of an offshore wind turbine component."@en .

ex:StructuralInstability a owl:Class ;
    rdfs:subClassOf ex:Failure ;
    rdfs:label "Inestabilidad estructural"@es ;
    rdfs:label "Structural Instability"@en ;
    rdfs:comment "Fallo que compromete la estabilidad estructural de la plataforma flotante."@es ;
    rdfs:comment "Failure that compromises the structural stability of the floating platform."@en .

##########################################################
# Measured data
##########################################################
ex:Measurement a owl:Class ;
    rdfs:subClassOf ex:Data ;
    rdfs:label "Medición"@es ;
    rdfs:label "Measurement"@en ;
    rdfs:comment "Representa una medición tomada por un sensor."@es ;
    rdfs:comment "Represents a measurement taken by a sensor."@en .

ex:Image a owl:Class ;
    rdfs:subClassOf ex:Data ;
    rdfs:label "Imagen"@es ;
    rdfs:label "Image"@en ;
    rdfs:comment "Una clase que representa una imagen que soporta la medición."@es ;
    rdfs:comment "A class that represents an image that supports the measurement."@en .

ex:VibrationData a owl:Class ;
    rdfs:subClassOf ex:Measurement ;
    rdfs:label "Datos de vibración"@es ;
    rdfs:label "Vibration Data"@en ;
    rdfs:comment "Datos de vibracion medidos por un sensor."@es ;
    rdfs:comment "Vibration data measured by a sensor."@en .

ex:StressData a owl:Class ;
    rdfs:subClassOf ex:Measurement ;
    rdfs:label "Datos de tensión"@es ;
    rdfs:label "Stress Data"@en ;
    rdfs:comment "Datos de tensión medidos por un sensor."@es ;
    rdfs:comment "Stress data measured by a sensor."@en .


##########################################################
# Data properties
##########################################################
ex:externalShape a owl:DatatypeProperty ;
    rdfs:domain ex:Component ;
    rdfs:range xsd:string ;
    rdfs:label "Forma externa"@es ;
    rdfs:label "External shape"@en ;
    rdfs:comment "Describe la forma externa o geometría del componente."@es ;
    rdfs:comment "Describe the external shape or geometric of the component."@en .

ex:length a owl:ObjectProperty ;
    rdfs:domain ex:Component ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Longitud"@es ;
    rdfs:label "Length"@en ;
    rdfs:comment "Longitud de un componente, expresada como un valor con unidad QUDT"@es ;
    rdfs:comment "Length of a component, expressed as a QUDT quantity value"@en .

ex:mass a owl:ObjectProperty ;
    rdfs:domain ex:Component ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Masa"@es ;
    rdfs:label "Mass"@en ;
    rdfs:comment "Masa del componente expresada expresada como un valor con unidad QUDT"@es ;
    rdfs:comment "Mass of the component expressed as a QUDT quantity value"@en .

ex:rotationSpeed a owl:ObjectProperty ;
    rdfs:domain ex:Component ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Velocidad de rotación"@es ;
    rdfs:label "Rotation speed"@en ;
    rdfs:comment "Velocidad angular de un componente expresada como un valor con unidad QUDT."@es ;
    rdfs:comment "Angular speed of a component expressed as a QUDT quantity value."@en .

ex:diameter a owl:ObjectProperty ;
    rdfs:domain ex:Component ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Diámetro"@es ;
    rdfs:label "Diameter"@en ;
    rdfs:comment "Diámetro del componente medido en unidades QUDT."@es ;
    rdfs:comment "Diameter of the component measured in QUDT units."@en .

ex:thickness a owl:ObjectProperty ;
    rdfs:domain ex:Component ;
    rdfs:range qudt:QuantityValue ;
    rdfs:label "Espesor"@es ;
