@prefix optimar: <https://w3id.org/optimar#> .
@prefix sosa: <https://www.w3.org/ns/sosa/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

##########################################################
# Platform and Location
##########################################################

optimar:NorthSeaLocation a optimar:Location ;
    rdfs:label "Mar del Norte"@es ;
    rdfs:label "North Sea"@en .

optimar:OptimarPlatform01 a optimar:FloatingPlatform ;
    rdfs:label "Plataforma Flotante OPTiMAR 01"@es ;
    rdfs:label "OPTiMAR Floating Platform 01"@en ;
    optimar:locatedIn optimar:NorthSeaLocation .

##########################################################
# Structural Components (Floating & Mooring)
##########################################################

optimar:FloatingSubstructure01 a optimar:FloatingSubstructure ;
    rdfs:label "Subestructura flotante 01"@es ;
    optimar:material "Acero Naval"@es ;
    optimar:supports optimar:Tower01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal .

optimar:MooringSystem01 a optimar:MooringSystem ;
    rdfs:label "Sistema de amarre 01"@es ;
    optimar:connectedTo optimar:FloatingSubstructure01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal .

##########################################################
# Turbine and its main components
##########################################################

optimar:Turbine01 a optimar:Turbine ;
    rdfs:label "Turbina Eólica 01"@es ;
    optimar:operationalStatus "Operativa"@es ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    
    # Propiedades QUDT (QuantityValue)
    optimar:hasCapacity [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "15"^^xsd:decimal ; 
        qudt:unit unit:MegaW 
    ] ;
    
    # Relación con sus componentes principales
    optimar:hasTurbineComponent optimar:Tower01, optimar:Nacelle01, optimar:Hub01, 
                                 optimar:Blade01, optimar:Blade02, optimar:Blade03 .

optimar:Tower01 a optimar:Tower ;
    rdfs:label "Torre T1"@es ;
    optimar:height [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "120"^^xsd:decimal ; 
        qudt:unit unit:M 
    ] ;
    optimar:material "Acero estructural S355"@es ;
    optimar:isSupportedBy optimar:FloatingSubstructure01 ;
    optimar:supports optimar:Nacelle01 ;
    optimar:hasOperationalState optimar:OperationalState_Degraded ;
    optimar:affectedBy optimar:TowerCorrosionFailure .

optimar:Nacelle01 a optimar:Nacelle ;
    rdfs:label "Nacelle T1"@es ;
    optimar:houses optimar:Generator01, optimar:Gearbox01 ;
    optimar:isSupportedBy optimar:Tower01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    optimar:temperatureMonitoredBy optimar:TempSensor_Nacelle ;
    # [NUEVO v2.2] Relación inversa con el sensor de Yaw
    optimar:yawAngleMonitoredBy optimar:YawSensor01 .

optimar:Generator01 a optimar:Generator ;
    rdfs:label "Generador T1"@es ;
    optimar:mass [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "35000"^^xsd:decimal ; 
        qudt:unit unit:KiloGM 
    ] ;
    optimar:isHousedIn optimar:Nacelle01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    optimar:temperatureMonitoredBy optimar:TempSensor_Generator ;
    optimar:electricalParameterMonitoredBy optimar:PowerSensor01, optimar:VoltageSensor01, 
                                           optimar:CurrentSensor01, optimar:FreqSensor01 .

optimar:Gearbox01 a optimar:Gearbox ;
    rdfs:label "Caja de cambios T1"@es ;
    optimar:material "Acero aleado"@es ;
    optimar:isHousedIn optimar:Nacelle01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    optimar:temperatureMonitoredBy optimar:TempSensor_Gearbox .

optimar:Transformer01 a optimar:Transformer ;
    rdfs:label "Transformador T1"@es ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    optimar:temperatureMonitoredBy optimar:TempSensor_Transformer .

optimar:Hub01 a optimar:Hub ;
    rdfs:label "Hub T1"@es ;
    optimar:connectedTo optimar:Nacelle01 ;
    optimar:transmitsMovementTo optimar:Generator01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    optimar:rotorParameterMonitoredBy optimar:RotorSpeedSensor01, optimar:RotorAngleSensor01 .

optimar:Blade01 a optimar:Blade ;
    rdfs:label "Pala 01"@es ;
    optimar:externalShape "Aerodinámica"@es ;
    optimar:length [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "90"^^xsd:decimal ; 
        qudt:unit unit:M 
    ] ;
    optimar:material "Fibra de Carbono"@es ;
    optimar:connectedTo optimar:Hub01 ;
    optimar:hasOperationalState optimar:OperationalState_Degraded ;
    optimar:affectedBy optimar:BladeCorrosionFailure ;
    # [NUEVO v2.2] Relación inversa con el sensor de Pitch
    optimar:pitchAngleMonitoredBy optimar:PitchSensor01 .

optimar:Blade02 a optimar:Blade ;
    rdfs:label "Pala 02"@es ;
    optimar:connectedTo optimar:Hub01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    # [NUEVO v2.2] Relación inversa con el sensor de Pitch
    optimar:pitchAngleMonitoredBy optimar:PitchSensor02 .

optimar:Blade03 a optimar:Blade ;
    rdfs:label "Pala 03"@es ;
    optimar:connectedTo optimar:Hub01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal ;
    # [NUEVO v2.2] Relación inversa con el sensor de Pitch
    optimar:pitchAngleMonitoredBy optimar:PitchSensor03 .

optimar:PitchSystem01 a optimar:PitchSystem ;
    rdfs:label "Sistema de Pitch Pala 01"@es ;
    optimar:connectedTo optimar:Blade01 ;
    optimar:hasOperationalState optimar:OperationalState_Normal .

##########################################################
# Operational States
##########################################################

optimar:OperationalState_Normal a optimar:OperationalState ;
    rdfs:label "Estado: Operativo Normal"@es .

optimar:OperationalState_Degraded a optimar:OperationalState ;
    rdfs:label "Estado: Degradado"@es .

optimar:OperationalState_Stopped a optimar:OperationalState ;
    rdfs:label "Estado: Detenido"@es .

optimar:OperationalState_Maintenance a optimar:OperationalState ;
    rdfs:label "Estado: En Mantenimiento"@es .

##########################################################
# Sensors - Monitoring Systems
##########################################################

# --- Sensores de Viento, Temperatura y Eléctricos (Existentes) ---
optimar:Anemometer01 a optimar:Anemometer ;
    rdfs:label "Anemómetro Principal"@es ;
    optimar:measuresWind optimar:CurrentWindCondition .

optimar:TempSensor_Ambient a optimar:TemperatureSensor ;
    rdfs:label "Sensor de Temperatura Ambiente"@es ;
    optimar:measuresTemperatureOf optimar:CurrentWindCondition .

optimar:TempSensor_Generator a optimar:TemperatureSensor ;
    rdfs:label "Sensor de Temperatura - Generador"@es ;
    optimar:measuresTemperatureOf optimar:Generator01 .

optimar:TempSensor_Gearbox a optimar:TemperatureSensor ;
    rdfs:label "Sensor de Temperatura - Gearbox"@es ;
    optimar:measuresTemperatureOf optimar:Gearbox01 .

optimar:TempSensor_Transformer a optimar:TemperatureSensor ;
    rdfs:label "Sensor de Temperatura - Transformador"@es ;
    optimar:measuresTemperatureOf optimar:Transformer01 .

optimar:TempSensor_Nacelle a optimar:TemperatureSensor ;
    rdfs:label "Sensor de Temperatura - Nacelle"@es ;
    optimar:measuresTemperatureOf optimar:Nacelle01 .

optimar:PowerSensor01 a optimar:ActivePowerSensor ;
    rdfs:label "Sensor de Potencia Activa"@es .

optimar:ReactivePowerSensor01 a optimar:ReactivePowerSensor ;
    rdfs:label "Sensor de Potencia Reactiva"@es .

optimar:VoltageSensor01 a optimar:VoltageSensor ;
    rdfs:label "Sensor de Voltaje - Generador"@es .

optimar:CurrentSensor01 a optimar:CurrentSensor ;
    rdfs:label "Sensor de Corriente - Generador"@es .

optimar:FreqSensor01 a optimar:GridFrequencySensor ;
    rdfs:label "Sensor de Frecuencia de Red"@es .

optimar:TorqueSensor01 a optimar:TorqueSensor ;
    rdfs:label "Sensor de Torque - Eje Principal"@es .

optimar:RotorSpeedSensor01 a optimar:RotorSpeedSensor ;
    rdfs:label "Sensor de Velocidad de Rotor"@es .

optimar:RotorAngleSensor01 a optimar:RotorAngleSensor ;
    rdfs:label "Sensor de Ángulo de Rotor"@es .

# --- [NUEVO v2.2] Sensores de Control (Yaw & Pitch) ---

optimar:YawSensor01 a optimar:YawSensor ;
    rdfs:label "Sensor de Orientación (Yaw) Nacelle"@es ;
    # Relación directa definida en v2.2
    optimar:monitorsYawOf optimar:Nacelle01 ;
    optimar:measuresYawAngle optimar:YawAngleData01 .

optimar:PitchSensor01 a optimar:PitchAngleSensor ;
    rdfs:label "Sensor de Pitch - Pala 01"@es ;
    # Relación directa definida en v2.2
    optimar:monitorsPitchOf optimar:Blade01 ;
    optimar:measuresPitchAngle optimar:PitchAngleData01 .

optimar:PitchSensor02 a optimar:PitchAngleSensor ;
    rdfs:label "Sensor de Pitch - Pala 02"@es ;
    optimar:monitorsPitchOf optimar:Blade02 ;
    optimar:measuresPitchAngle optimar:PitchAngleData02 .

optimar:PitchSensor03 a optimar:PitchAngleSensor ;
    rdfs:label "Sensor de Pitch - Pala 03"@es ;
    optimar:monitorsPitchOf optimar:Blade03 ;
    optimar:measuresPitchAngle optimar:PitchAngleData03 .

# --- Sensores de corrosión y estructura ---
optimar:CorrosionCam01 a optimar:CorrosionCamera ;
    rdfs:label "Cámara de corrosión - Pala 01"@es ;
    sosa:observes optimar:Blade01 ;
    optimar:detectsCorrosion optimar:BladeCorrosionFailure .

optimar:CorrosionCam02 a optimar:CorrosionCamera ;
    rdfs:label "Cámara de corrosión - Torre"@es ;
    sosa:observes optimar:Tower01 ;
    optimar:detectsCorrosion optimar:TowerCorrosionFailure .

optimar:VibrationSensor01 a optimar:DynamicMonitor ;
    rdfs:label "Sensor de vibración de Torre"@es ;
    optimar:measuresVibration optimar:TowerVibrationData01 .

optimar:VibrationSensor02 a optimar:DynamicMonitor ;
    rdfs:label "Sensor de vibración de Nacelle"@es ;
    optimar:measuresVibration optimar:NacelleVibrationData01 .

optimar:GaugeSystem01 a optimar:GaugeSystem ;
    rdfs:label "Sistema de galgas - Torre"@es ;
    optimar:measuresStress optimar:TowerStressData01 .

##########################################################
# Actors and Roles
##########################################################

optimar:Operator01 a optimar:MaintenanceOperator ;
    rdfs:label "Juan Pérez"@es ;
    optimar:hasRole optimar:TechnicalRole ;
    optimar:monitors optimar:Anemometer01, optimar:YawSensor01 ;
    optimar:performsTask optimar:RoutineInspection01 .

optimar:Analyst01 a optimar:DataAnalyst ;
    rdfs:label "Ana García"@es ;
    optimar:hasRole optimar:AnalyticalRole ;
    optimar:analyzes optimar:CorrosionEvent01, optimar:BladeCorrosionFailure, optimar:YawAngleData01 .

optimar:CloudAdmin01 a optimar:CloudAdministrator ;
    rdfs:label "Admin Cloud"@es ;
    optimar:hasRole optimar:ManagementRole ;
    optimar:manages optimar:OptimarPlatform01 .

optimar:ParkOwner01 a optimar:ParkOwner ;
    rdfs:label "OPTiMAR Energy Solutions"@es ;
    optimar:hasRole optimar:ManagementRole ;
    optimar:manages optimar:OptimarPlatform01 .

##########################################################
# Tasks and Maintenance
##########################################################

optimar:RoutineInspection01 a optimar:MonitoringTask ;
    rdfs:label "Inspección Rutinaria de Sensores"@es ;
    optimar:performedBy optimar:Operator01 .

optimar:BladeRepair01 a optimar:Maintenance ;
    rdfs:label "Reparación de superficie de Pala 01"@es ;
    optimar:performedBy optimar:Operator01 ;
    optimar:startDateTime "2026-01-20T08:00:00Z"^^xsd:dateTime ;
    optimar:endDateTime "2026-01-20T16:30:00Z"^^xsd:dateTime ;
    optimar:duration [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "8.5"^^xsd:decimal ; 
        qudt:unit unit:HR 
    ] .

optimar:TowerInspection01 a optimar:Maintenance ;
    rdfs:label "Inspección estructural de Torre"@es ;
    optimar:performedBy optimar:Operator01 ;
    optimar:startDateTime "2026-01-22T09:00:00Z"^^xsd:dateTime ;
    optimar:endDateTime "2026-01-22T12:00:00Z"^^xsd:dateTime ;
    optimar:duration [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "3"^^xsd:decimal ; 
        qudt:unit unit:HR 
    ] .

##########################################################
# Failures
##########################################################

optimar:BladeCorrosionFailure a optimar:Corrosion ;
    rdfs:label "Corrosión por salinidad - Pala 01"@es ;
    optimar:severity "medium"^^xsd:string ;
    optimar:detectionDateTime "2026-01-15T14:30:00Z"^^xsd:dateTime ;
    optimar:affects optimar:Blade01 ;
    optimar:hasFailureCause optimar:CurrentWindCondition ;
    optimar:causesFailure optimar:BladeFatigueFailure .

optimar:BladeFatigueFailure a optimar:MaterialFatigue ;
    rdfs:label "Fatiga material - Pala 01"@es ;
    optimar:severity "high"^^xsd:string ;
    optimar:detectionDateTime "2026-01-18T10:15:00Z"^^xsd:dateTime ;
    optimar:affects optimar:Blade01 ;
    optimar:precededBy optimar:BladeCorrosionFailure .

optimar:TowerCorrosionFailure a optimar:Corrosion ;
    rdfs:label "Corrosión base de torre"@es ;
    optimar:severity "low"^^xsd:string ;
    optimar:detectionDateTime "2026-01-16T11:00:00Z"^^xsd:dateTime ;
    optimar:affects optimar:Tower01 ;
    optimar:hasFailureCause optimar:CurrentWindCondition .

##########################################################
# Failure Events
##########################################################

optimar:CorrosionEvent01 a optimar:CorrosionEvent ;
    rdfs:label "Evento de detección de corrosión #234"@es ;
    optimar:causedBy optimar:BladeCorrosionFailure ;
    optimar:detectedBy optimar:CorrosionCam01 ;
    optimar:relatedToComponent optimar:Blade01 ;
    optimar:startDateTime "2026-01-15T14:30:00Z"^^xsd:dateTime ;
    optimar:endDateTime "2026-01-15T14:35:00Z"^^xsd:dateTime ;
    optimar:duration [ 
        a qudt:QuantityValue ; 
        qudt:numericValue "5"^^xsd:decimal ; 
        qudt:unit unit:MIN 
    ] ;
    optimar:corrosionLevel "0.45"^^xsd:decimal ;
    optimar:darkCorrosionLevel "0.30"^^xsd:decimal ;
    optimar:lightCorrosion "0.15"^^xsd:decimal ;
    prov:generated optimar:CorrosionImage01 .

optimar:CorrosionEvent02 a optimar:CorrosionEvent ;
    rdfs:label "Evento de detección de corrosión en torre #235"@es ;
    optimar:causedBy optimar:TowerCorrosionFailure ;
    optimar:detectedBy optimar:CorrosionCam02 ;
    optimar:relatedToComponent optimar:Tower01 ;
    optimar:startDateTime "2026-01-16T11:00:00Z"^^xsd:dateTime ;
    optimar:corrosionLevel "0.25"^^xsd:decimal ;
    prov:generated optimar:CorrosionImage02 .

